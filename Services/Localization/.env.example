# HelixTrack Localization Service - Environment Variables Example
# Copy this file to .env and update with your actual values

# Database Configuration
LOCALIZATION_DB_NAME=helixtrack_localization
LOCALIZATION_DB_USER=localization_user
LOCALIZATION_DB_PASSWORD=localization_secure_password_change_me
LOCALIZATION_DB_PORT=5434

# Redis Configuration (optional)
REDIS_PASSWORD=redis_password_change_me
REDIS_PORT=6380

# Service Configuration
LOCALIZATION_PORT=8085
JWT_SECRET=your-jwt-secret-key-change-in-production

# Logging
LOG_LEVEL=info

# Backup Configuration
BACKUP_DIR=./backups
COMPRESS=true

# Notification Webhook (optional)
NOTIFICATION_WEBHOOK=
