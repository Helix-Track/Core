{
  "service": {
    "id": "helixtrack-auth-default",
    "name": "helixtrack-auth",
    "tags": [
      "auth",
      "authentication",
      "jwt",
      "v1.0.0"
    ],
    "address": "auth-service",
    "port": 8081,
    "meta": {
      "version": "1.0.0",
      "environment": "production",
      "service_type": "authentication",
      "supports_rotation": "true",
      "auto_port_selection": "true"
    },
    "checks": [
      {
        "id": "helixtrack-auth-health",
        "name": "HelixTrack Auth Health Check",
        "http": "http://auth-service:8081/health",
        "method": "GET",
        "interval": "30s",
        "timeout": "10s",
        "deregister_critical_service_after": "90s",
        "success_before_passing": 2,
        "failures_before_critical": 3
      },
      {
        "id": "helixtrack-auth-tcp",
        "name": "HelixTrack Auth TCP Check",
        "tcp": "auth-service:8081",
        "interval": "30s",
        "timeout": "5s"
      }
    ],
    "enable_tag_override": false,
    "weights": {
      "passing": 10,
      "warning": 1
    }
  }
}
